<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tyj.jhpt.dao.DeviceGpsInfoMapper">
    <select id="findPageMsg" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from `tb_device_gps_info`
        <where>
            <if test="startDate != null and startDate != '' and endDate != null and endDate != ''">
                AND `event_time` &gt;= #{startDate}
                AND `event_time` &lt;= #{endDate}
            </if>
            <if test="carVin != null and carVin != ''">
                AND `car_vin` = #{carVin}
            </if>
            <if test="map != null">
                AND `longitude` > 0 AND `latitude` > 0
            </if>
            <if test="msgType != null">
                AND `msg_type` = #{msgType}
            </if>
        </where>
        ORDER BY `event_time` desc
    </select>

    <select id="findMsg" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from `tb_device_gps_info`
        <where>
            <if test="startDate != null and startDate != '' and endDate != null and endDate != ''">
                AND `event_time` &gt;= #{startDate}
                AND `event_time` &lt;= #{endDate}
            </if>
            <if test="carVin != null and carVin != ''">
                AND `car_vin` = #{carVin}
            </if>
            <if test="map != null">
                AND `longitude` > 0 AND `latitude` > 0
            </if>
            <if test="msgType != null">
                AND `msg_type` = #{msgType}
            </if>
        </where>
    </select>
</mapper>